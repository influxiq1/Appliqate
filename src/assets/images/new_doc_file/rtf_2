\pard \qj \li720 {\fs20 {Management currently is not taking cash compensation. The compensation was recorded as additional paid in capital. It has been agreed between management that this will continue until revenues of the company increase.}}\par
\pard \qj {\fs20 {\u160?}}\par
{\*\bkmkstart PARTIIANDIII_HTM_A_012}{\*\bkmkend PARTIIANDIII_HTM_A_012}\pard \qc {\fs20 {{\b  SECURITY OWNERSHIP OF MANAGEMENT AND CERTAIN SECURITY HOLDERS}}}\par
\pard \qj {\fs20 {\u160?}}\par
\pard \qj\fi720 {\fs20 {The following table sets forth information as to the shares of common stock beneficially owned as of Jan. 31, 2016 by (i) each person known to us to be the beneficial owner of more than 5% of our common stock; (ii) each Director; (iii) each Executive Officer; and (iv) all of our Directors and Executive Officers as a group. Unless otherwise indicated in the footnotes following the table, the persons as to whom the information is given had sole voting and investment power over the shares of common stock shown as beneficially owned by them.}}\par
\pard \fi720 {\fs20 {\u160?}}{\*\bkmkstart FIS_UNIDENTIFIED_TABLE_7}{\*\bkmkend FIS_UNIDENTIFIED_TABLE_7}\par
{\trowd\trleft0\clbrdrb\brdrs\brdrw30\brdrcf0\clvertalb\cellx7551 \clvertalb\cellx7793 \clbrdrb\brdrs\brdrw30\brdrcf0\clvertalb\cellx9299 \clvertalb\cellx9432 \clvertalb\cellx9674 \clbrdrb\brdrs\brdrw30\brdrcf0\clvertalb\cellx11387 \clvertalb\cellx11520 \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub Directors and Executive Officers}}\cell \pard \intbl\vertalb \ri16 {\fs20\b {\nosupersub \u160?}}\cell \pard \intbl\qc\vertalb \ri36 {\fs20\b {\nosupersub Amount}}\cell \pard \intbl\vertalb \ri16 {\fs20\b {\nosupersub \u160?}}\cell \pard \intbl\vertalb \ri16 {\fs20\b {\nosupersub \u160?}}\cell \pard \intbl\qc\vertalb \ri36 {\fs20\b {\nosupersub Percent}}\cell \pard \intbl\vertalb \ri16 {\fs20\b {\nosupersub \u160?}}\cell \row}
{\trowd\trleft0\clvertalb\cellx7551 \clvertalb\cellx7793 \clvertalb\cellx7926 \clvertalb\cellx9299 \clvertalb\cellx9432 \clvertalb\cellx9674 \clvertalb\cellx10032 \clvertalb\cellx11387 \clvertalb\cellx11520 \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub David Anderson}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\qr\vertalb \ri16 {\fs20 {\nosupersub 20,000,000}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\qr\vertalb \ri16 {\fs20 {\nosupersub 33}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \row}
{\trowd\trleft0\clvertalb\clcbpat4\cellx7551 \clvertalb\clcbpat4\cellx7793 \clvertalb\clcbpat4\cellx7926 \clvertalb\clcbpat4\cellx9299 \clvertalb\clcbpat4\cellx9432 \clvertalb\clcbpat4\cellx9674 \clvertalb\clcbpat4\cellx10032 \clvertalb\clcbpat4\cellx11387 \clvertalb\clcbpat4\cellx11520 \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub Witaly Yatsik}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\qr\vertalb \ri16 {\fs20 {\nosupersub 20,000,000}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\qr\vertalb \ri16 {\fs20 {\nosupersub 33}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \row}
{\trowd\trleft0\clvertalb\cellx7551 \clvertalb\cellx7793 \clvertalb\cellx7926 \clvertalb\cellx9299 \clvertalb\cellx9432 \clvertalb\cellx9674 \clvertalb\cellx10032 \clvertalb\cellx11387 \clvertalb\cellx11520 \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub Jordan Hemming}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\qr\vertalb \ri16 {\fs20 {\nosupersub 20,000,000}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \pard \intbl\qr\vertalb \ri16 {\fs20 {\nosupersub 33}}\cell \pard \intbl\vertalb \ri16 {\fs20 {\nosupersub \u160?}}\cell \row}
