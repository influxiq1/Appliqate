\pard \qj {\fs20 {\u160?}}{\*\bkmkstart FIS_UNIDENTIFIED_TABLE_6}{\*\bkmkend FIS_UNIDENTIFIED_TABLE_6}\par
\pard \qc {\fs20 {{\b Executive Compensation}}}\par
\pard \fi720 {\fs20 {\u160?}}\par
{\trowd\trleft0\clbrdrb\brdrs\brdrw30\brdrcf0\clvertalb\cellx6390 \clvertalb\cellx6632 \clbrdrb\brdrs\brdrw30\brdrcf0\clvertalb\cellx9091 \clvertalb\cellx9333 \clbrdrb\brdrs\brdrw30\brdrcf0\clvertalb\cellx11387 \clvertalb\cellx11520 \pard \intbl\qj\vertalb \ri16 {\fs20 {\nosupersub Management}}\cell \pard \intbl\vertalb \ri16 {\fs20\b {\nosupersub \u160?}}\cell \pard \intbl\qc\vertalb \ri16 {\fs20\b {\nosupersub Position}}\cell \pard \intbl\vertalb \ri16 {\fs20\b {\nosupersub \u160?}}\cell \pard \intbl\qc\vertalb \ri36 {\fs20\b {\nosupersub Compensation}}\cell \pard \intbl\vertalb \ri16 {\fs20\b {\nosupersub \u160?}}\cell \row}
